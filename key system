-- üìå –•—Ä–∞–Ω–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –æ –∫–ª—é—á–∞—Ö (–≤ —Ä–µ–∞–ª—å–Ω–æ–π —Å–∏—Å—Ç–µ–º–µ, –¥–∞–Ω–Ω—ã–µ –º–æ–∂–Ω–æ —Ö—Ä–∞–Ω–∏—Ç—å –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ –∏–ª–∏ –≤ –±–∞–∑–µ –¥–∞–Ω–Ω—ã—Ö)
local keyData = {
    [""] = {
        activationTime = os.time(),  -- –í—Ä–µ–º—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –∫–ª—é—á–∞
        usedUserIds = {},  -- –°–ø–∏—Å–æ–∫ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–Ω—ã—Ö UserId
        maxDevices = 90  -- –ú–∞–∫—Å–∏–º—É–º 90 —É—Å—Ç—Ä–æ–π—Å—Ç–≤
    },
    -- –î–æ–±–∞–≤—å—Ç–µ –¥—Ä—É–≥–∏–µ –∫–ª—é—á–∏ —Å –¥–∞–Ω–Ω—ã–º–∏
}

-- üìå –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è UserId –∏–≥—Ä–æ–∫–∞ (–≤–º–µ—Å—Ç–æ HWID)
local function getUserId()
    return game.Players.LocalPlayer.UserId  -- –ü–æ–ª—É—á–∞–µ–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–π –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –∏–≥—Ä–æ–∫–∞
end

-- üìå –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞
local function checkKey(inputKey)
    local playerId = getUserId()  -- –ü–æ–ª—É—á–∞–µ–º UserId —Ç–µ–∫—É—â–µ–≥–æ –∏–≥—Ä–æ–∫–∞
    local currentTime = os.time()

    if keyData[inputKey] then
        local keyInfo = keyData[inputKey]
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ä–æ–∫–∞ –¥–µ–π—Å—Ç–≤–∏—è –∫–ª—é—á–∞ (30 –¥–Ω–µ–π)
        if currentTime - keyInfo.activationTime > 30 * 24 * 60 * 60 then
            return false, "–ö–ª—é—á –ø—Ä–æ—Å—Ä–æ—á–µ–Ω!"  -- –ö–ª—é—á –ø—Ä–æ—Å—Ä–æ—á–µ–Ω
        end

        -- –ü—Ä–æ–≤–µ—Ä–∫–∞, –µ—Å–ª–∏ –∫–ª—é—á —É–∂–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–ª—Å—è –Ω–∞ 90 —É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞—Ö
        if #keyInfo.usedUserIds >= keyInfo.maxDevices then
            return false, "ERROR: You are a regular buyer. Buyers have access to run only 90 accounts. Buy 1 more key or buy Prem Buyer."
        end

        -- –ü—Ä–æ–≤–µ—Ä–∫–∞, –µ—Å–ª–∏ —ç—Ç–æ—Ç –∫–ª—é—á —É–∂–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–ª—Å—è –Ω–∞ —ç—Ç–æ–º UserId
        for _, userId in ipairs(keyInfo.usedUserIds) do
            if userId == playerId then
                return false, "–ö–ª—é—á —É–∂–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–ª—Å—è –Ω–∞ –≤–∞—à–µ–º –∞–∫–∫–∞—É–Ω—Ç–µ!"  -- –ö–ª—é—á —É–∂–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω
            end
        end

        -- –î–æ–±–∞–≤–ª—è–µ–º UserId –≤ —Å–ø–∏—Å–æ–∫ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–Ω—ã—Ö
        table.insert(keyInfo.usedUserIds, playerId)

        return true, "–ö–ª—é—á –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª–µ–Ω!"
    else
        return false, "–ù–µ–≤–µ—Ä–Ω—ã–π –∫–ª—é—á!"  -- –ö–ª—é—á –Ω–µ –Ω–∞–π–¥–µ–Ω
    end
end

-- üìå –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –¥–ª—è –≤–≤–æ–¥–∞ –∫–ª—é—á–∞
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer.PlayerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 150)
frame.Position = UDim2.new(0.5, -150, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
frame.Parent = screenGui

local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, 0, 0.2, 0)
titleLabel.Position = UDim2.new(0, 0, 0, 0)
titleLabel.Text = "Enter Key:"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
titleLabel.BackgroundTransparency = 1
titleLabel.TextSize = 24
titleLabel.TextScaled = true
titleLabel.Parent = frame

local keyInput = Instance.new("TextBox")
keyInput.Size = UDim2.new(0, 260, 0, 50)
keyInput.Position = UDim2.new(0, 20, 0, 40)
keyInput.Text = ""
keyInput.PlaceholderText = "Enter your key here"
keyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
keyInput.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
keyInput.TextSize = 24
keyInput.Parent = frame

local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0, 260, 0, 40)
submitButton.Position = UDim2.new(0, 20, 0, 100)
submitButton.Text = "Submit"
submitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
submitButton.BackgroundColor3 = Color3.fromRGB(0, 128, 0)
submitButton.TextSize = 24
submitButton.Parent = frame

-- üìå –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –≤–≤–æ–¥–∞ –∫–ª—é—á–∞
submitButton.MouseButton1Click:Connect(function()
    local inputKey = keyInput.Text  -- –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—Å—Ç –∏–∑ –ø–æ–ª—è –≤–≤–æ–¥–∞

    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞
    local isValid, message = checkKey(inputKey)
    if isValid then
        print(message)
        screenGui:Destroy()  -- –£–±–∏—Ä–∞–µ–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å, –µ—Å–ª–∏ –∫–ª—é—á –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π

        -- –ó–¥–µ—Å—å –∏–¥–µ—Ç –æ—Å–Ω–æ–≤–Ω–æ–π –∫–æ–¥ —Å–∫—Ä–∏–ø—Ç–∞, –∫–æ—Ç–æ—Ä—ã–π –±—É–¥–µ—Ç –≤—ã–ø–æ–ª–Ω—è—Ç—å—Å—è –ø–æ—Å–ª–µ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–≥–æ –∫–ª—é—á–∞
        -- –ü—Ä–∏–º–µ—Ä:
        runAsync(function()
            -- –û—Å–Ω–æ–≤–Ω–æ–π –∫–æ–¥...
        end)
    else
        game.Players.LocalPlayer:Kick(message)  -- –û—Ç–∫–ª—é—á–∞–µ–º –∏–≥—Ä–æ–∫–∞ —Å —Å–æ–æ–±—â–µ–Ω–∏–µ–º
    end
end)
